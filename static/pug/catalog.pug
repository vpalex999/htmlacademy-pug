extends layout/layout

block content
  main.catalog-page
    section.breadcrumbs
      .container
        ul.breadcrumbs__list
          li.breadcrumbs__item
            a.breadcrumbs__link(href="index.html") Главная страница
          li.breadcrumbs__item
            span.breadcrumbs__text Каталог книг
    section.catalog
      .container
        h1.catalog__title Каталог книг
        .catalog__wrapper
          .catalog-filter
            form.catalog-filter__form
              fieldset.catalog-filter__fieldset
                p.catalog-filter__legend Категория
                .catalog-filter__box
                  input#fiction.catalog-filter__checkbox(
                    type="checkbox"
                    name="fiction"
                  )
                  label.catalog-filter__label(for="fiction") Художественная литература
                .catalog-filter__box
                  input#for-children.catalog-filter__checkbox(
                    type="checkbox"
                    name="for-children"
                  )
                  label.catalog-filter__label(for="for-children") Книги для детей
                .catalog-filter__box
                  input#education.catalog-filter__checkbox(
                    type="checkbox"
                    name="education"
                    checked
                  )
                  label.catalog-filter__label(for="education") Образование
                .catalog-filter__box
                  input#science-and-technology.catalog-filter__checkbox(
                    type="checkbox"
                    name="science-and-technology"
                  )
                  label.catalog-filter__label(for="science-and-technology") Наука и техника
                .catalog-filter__box
                  input#society.catalog-filter__checkbox(
                    type="checkbox"
                    name="society"
                  )
                  label.catalog-filter__label(for="society") Общество
                .catalog-filter__box
                  input#business-literature.catalog-filter__checkbox(
                    type="checkbox"
                    name="business-literature"
                  )
                  label.catalog-filter__label(for="business-literature") Деловая литература
                .catalog-filter__box
                  input#health.catalog-filter__checkbox(
                    type="checkbox"
                    name="health"
                  )
                  label.catalog-filter__label(for="health") Здоровье
                .catalog-filter__box
                  input#hobby.catalog-filter__checkbox(
                    type="checkbox"
                    name="hobby"
                    checked
                  )
                  label.catalog-filter__label(for="hobby") Увлечения
                .catalog-filter__box
                  input#psychology.catalog-filter__checkbox(
                    type="checkbox"
                    name="psychology"
                  )
                  label.catalog-filter__label(for="psychology") Психология
                .catalog-filter__box
                  input#esoterics.catalog-filter__checkbox(
                    type="checkbox"
                    name="esoterics"
                  )
                  label.catalog-filter__label(for="esoterics") Эзотерика
                .catalog-filter__box
                  input#philosophy.catalog-filter__checkbox(
                    type="checkbox"
                    name="philosophy"
                  )
                  label.catalog-filter__label(for="philosophy") Философия и религия
                .catalog-filter__box
                  input#art.catalog-filter__checkbox(
                    type="checkbox"
                    name="art"
                    checked
                  )
                  label.catalog-filter__label(for="art") Искусство
              fieldset.catalog-filter__fieldset
                p.catalog-filter__legend Статус
                .catalog-filter__box.catalog-filter__box--radio
                  input#bestsellers.catalog-filter__radio(
                    type="radio"
                    name="status"
                    value="bestsellers"
                  )
                  label.catalog-filter__label(for="bestsellers") Хиты продаж
                .catalog-filter__box.catalog-filter__box--radio
                  input#news-books.catalog-filter__radio(
                    type="radio"
                    name="status"
                    value="news-books"
                    checked
                  )
                  label.catalog-filter__label(for="news-books") Новинки
                .catalog-filter__box.catalog-filter__box--radio
                  input#preorder.catalog-filter__radio(
                    type="radio"
                    name="status"
                    value="preorder"
                  )
                  label.catalog-filter__label(for="preorder") Предзаказ
              fieldset.catalog-filter__fieldset
                p.catalog-filter__legend Наличие
                .catalog-filter__box.catalog-filter__box--radio
                  input#in-the-shop.catalog-filter__radio(
                    type="radio"
                    name="availability"
                    value="in-the-shop"
                  )
                  label.catalog-filter__label(for="in-the-shop") В магазине
                .catalog-filter__box.catalog-filter__box--radio
                  input#in-the-internet-shop.catalog-filter__radio(
                    type="radio"
                    name="availability"
                    value="in-the-internet-shop"
                    checked
                  )
                  labe.catalog-filter__label(for="in-the-internet-shop") В интернет-магазине
                .catalog-filter__box.catalog-filter__box--radio
                  input#under-the-order.catalog-filter__radio(
                    type="radio"
                    name="availability"
                    value="under-the-order"
                  )
                  label.catalog-filter__label(for="under-the-order") Под заказ
          .catalog-books
            .catalog-books__sort
              label.catalog-books__sort-title(for="sort") Сортировать:
              select#sort.catalog-books__selectsize="1"
                option.catalog-books__options по популярности
                option.catalog-books__options по названию
                option.catalog-books__options по новизне
            ul.books
              li.books__item
                .books__inner
                  img.books__img(
                    src="static/images/content/book-iron-king.jpg"
                    alt='Фото книги - "Фигурная абстракция"'
                    width="221"
                    height="221"
                  )
